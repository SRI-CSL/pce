sort S;
const a, b, c, d, e: S;
predicate P(S) indirect;
predicate Q(S) indirect;
add P(a) 2.0;
add P(c) 2.0;
add P(e) 2.0;
add P(b) or P(d) -2.0;
add ~P(a) or Q(a) 3.0;
add ~P(b) or Q(b) 3.0;
add ~P(c) or Q(c) 3.0;
add ~P(d) or Q(d) 3.0;
add ~P(e) or Q(e) 3.0;
mcsat_params 0.01, 20.0, 0.01, 30, 500;
mcsat;
