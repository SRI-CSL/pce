# tools
SED=@SED@
LN_S=@LN_S@
MKDIR_P=@MKDIR_P@

# compilation
#YACC=@YACC@
#YFLAGS=@YFLAGS@
YACC=bison
YFLAGS=-d -t
CC=@CC@
CFLAGS=@CFLAGS@
CPPFLAGS=@CPPFLAGS@
LIBS=@LIBS@
LDFLAGS=@LDFLAGS@
LD=@LD@
AR=@AR@
RANLIB=@RANLIB@
STRIP=@STRIP@
OAACFLAGS=@OAACFLAGS@
OAALDFLAGS=@OAALDFLAGS@
